    "sassImport": [
        "@wpmedia/news-theme-css/scss/_variables.scss",
        "@wpmedia/news-theme-css/scss/_functions.scss",
        "@wpmedia/news-theme-css/scss/_colors.scss",
        "@wpmedia/news-theme-css/scss/_breakpoints.scss",
        "@wpmedia/news-theme-css/scss/_mixins.scss"
    ],
    "sassVariableOverrides": [
        "$primary-font-family: $theme-primary-font-family;",
        "$secondary-font-family: $theme-primary-font-family;",
        "$primary-color: $theme-primary-color;"
    ],
    "values": {
        "default": {
            "siteProperties": {
                "feedTitle": "Outbound Feeds",
                "feedDomainURL": "<http://localhost>",
                "feedLanguage": "en",
                "resizerURL": "<http://localhost/resizer>",
                "feedDefaultQuery": [
                    {
                        "term":
                        {"type":"story"}
                    },
                    {
                        "range":
                        {
                            "display_date":
                            {
                                "gte": "now-2d",
                                "lte": "now"
                            }
                        }
                    }
                ]
            }
        },
        "sites": {
            "pyxis": {
                "siteProperties": {
                    "feedTitle": "website 1",
                    "feedDomainURL": "<https://www.website1.com>",
                    "feedLanguage": "en",
                    "resizerURL": "<https://www.website1.com/resizer>"                }
            }
        }
    }
}